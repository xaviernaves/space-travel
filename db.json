  "bodies": [
    {
      "id": 1,
      "name": "Mercury",
      "type": "planet",
      "star": "Sun",
      "description": "Mercury—the smallest planet in our solar system and closest to the Sun—is only slightly larger than Earth's Moon. Mercury is the fastest planet, zipping around the Sun every 88 Earth days.",
      "images": ["http://localhost:3000/img/bodies/mercury.png"]
    },
    {
      "id": 2,
      "name": "Venus",
      "type": "planet",
      "star": "Sun",
      "description": "Venus spins slowly in the opposite direction from most planets. A thick atmosphere traps heat in a runaway greenhouse effect, making it the hottest planet in our solar system.",
      "images": [
        "http://localhost:3000/img/bodies/venus.png"
      ]
    },
    {
      "id": 4,
      "name": "Mars",
      "type": "planet",
      "star": "Sun",
      "description": "Mars is a dusty, cold, desert world with a very thin atmosphere. There is strong evidence Mars was—billions of years ago—wetter and warmer, with a thicker atmosphere.",
      "images": [
        "http://localhost:3000/img/bodies/mars.png"
      ]
    },
    {
      "id": 5,
      "name": "Jupiter",
      "type": "planet",
      "star": "Sun",
      "description": "Jupiter is more than twice as massive than the other planets of our solar system combined. The giant planet's Great Red spot is a centuries-old storm bigger than Earth.",
      "images": [
        "http://localhost:3000/img/bodies/jupiter.png"
      ]
    },
    {
      "id": 6,
      "name": "Saturn",
      "type": "planet",
      "star": "Sun",
      "description": "Adorned with a dazzling, complex system of icy rings, Saturn is unique in our solar system. The other giant planets have rings, but none are as spectacular as Saturn's.",
      "images": [
        "http://localhost:3000/img/bodies/saturn.png"
      ]
    },
    {
      "id": 7,
      "name": "Uranus",
      "type": "planet",
      "star": "Sun",
      "description": "Uranus—seventh planet from the Sun—rotates at a nearly 90-degree angle from the plane of its orbit. This unique tilt makes Uranus appear to spin on its side.",
      "images": [
        "http://localhost:3000/img/bodies/uranus.png"
      ]
    },
    {
      "id": 8,
      "name": "Neptune",
      "type": "planet",
      "star": "Sun",
      "description": "Neptune—the eighth and most distant major planet orbiting our Sun—is dark, cold and whipped by supersonic winds. It was the first planet located through mathematical calculations, rather than by telescope.",
      "images": [
        "http://localhost:3000/img/bodies/neptune.png"
      ]
    },
    {
      "id": 14,
      "name": "Moon",
      "type": "moon",
      "star": "Earth",
      "description": "Earth's Moon makes our planet more livable by moderating how much it wobbles on its axis.",
      "images": ["http://localhost:3000/img/bodies/earths_moon.png"]
    },
    {
      "id": 15,
      "name": "Phobos",
      "type": "moon",
      "star": "Mars",
      "description": "Phobos is the larger of Mars' two moons and is 17 x 14 x 11 miles (27 by 22 by 18 kilometers) in diameter. It orbits Mars three times a day, and is so close to the planet's surface that in some locations on Mars it cannot always be seen.",
      "images": [
        "http://localhost:3000/img/bodies/mars_moon.png"
      ]
    },
    {
      "id": 18,
      "name": "Europa",
      "type": "moon",
      "star": "Jupiter",
      "description": "Slightly smaller than Earth's Moon, Europa’s water-ice surface is crisscrossed by long, linear fractures, cracks, ridges, and bands. The moon’s ice shell is probably 10 to 15 miles (15 to 25 kilometers) thick, beneath which the ocean is estimated to be 40 to 100 miles (60 to 150 kilometers) deep. Like Earth, Europa is thought to also contain a rocky mantle and iron core.",
      "images": ["http://localhost:3000/img/bodies/jupiters_moon.png"]
    }
